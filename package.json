{
  "name": "aiplanner-workspace",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "web",
    "mobile",
    "shared"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=web",
    "dev:web": "npm run dev --workspace=web",
    "dev:mobile": "npm run start --workspace=mobile",
    "android": "npm run android --workspace=mobile",
    "ios": "npm run ios --workspace=mobile",
    "build": "npm run build --workspace=web",
    "build:web": "npm run build --workspace=web",
    "build:mobile": "npm run build --workspace=mobile",
    "lint": "npm run lint --workspace=web && npm run lint --workspace=mobile",
    "preview": "npm run preview --workspace=web"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/react-query": "^5.74.4",
    "date-fns": "^4.1.0",
    "firebase": "^12.0.0",
    "flag-icons": "^7.5.0",
    "framer-motion": "^12.10.0",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.503.0",
    "msw": "^2.7.5",
    "react": "19.0.0",
    "react-country-flag": "^3.1.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.7.3",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.1",
    "tailwindcss": "^4.1.4",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/react": "~19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "path": "^0.12.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
